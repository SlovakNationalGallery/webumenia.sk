{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/rastislav-chynoransky/color-extractor.git",
        "no-api": true
    }],
    "require": {
        "php": "^8.0.2",
        "anahkiasen/former": "^4.0",
        "astrotomic/laravel-translatable": "^11.3",
        "babenkoivan/elastic-scout-driver": "^2.0",
        "babenkoivan/elastic-scout-driver-plus": "^3.4",
        "barryvdh/laravel-form-bridge": "^0.6",
        "caseyamcl/phpoaipmh": "dev-master",
        "chelout/laravel-relationship-events": "^1.0.0",
        "doctrine/dbal": "^2.5",
        "elasticsearch/elasticsearch": "^7.2",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^9.19",
        "laravel/legacy-factories": "^1.1",
        "laravel/scout": "^9.2",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.3",
        "league/color-extractor": "dev-master",
        "league/csv": "^8.0",
        "livewire/livewire": "^2.7",
        "mcamara/laravel-localization": "1.7.*",
        "primal/color": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "dev-master",
        "rtconner/laravel-tagging": "^4.1",
        "sentry/sentry-laravel": "^2.3",
        "spatie/laravel-googletagmanager": "^2.6",
        "spatie/laravel-medialibrary": "^9.7",
        "spatie/laravel-missing-page-redirector": "^2.7",
        "spatie/laravel-newsletter": "^4.10",
        "spatie/laravel-sitemap": "^6.2",
        "symfony/templating": "^5.2",
        "unisharp/laravel-filemanager": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "deployer/deployer": "v7.x",
        "fakerphp/faker": "^1.19",
        "filp/whoops": "~2.0",
        "laravel/browser-kit-testing": "^6.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "github-protocols": ["https"],
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/general.php",
            "app/Helpers/i18n.php",
            "app/Helpers/view.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=lfm_public"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
