{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.2.*",
        "caseyamcl/Phpoaipmh": "dev-master",
        "intervention/image": "dev-master",
        "guzzlehttp/guzzle": "dev-master",
        "fadion/bouncy": "dev-l5",
        "rtconner/laravel-tagging": "~2.1",
        "maatwebsite/excel": "~2.1.0",
        "rap2hpoutre/laravel-log-viewer": "dev-master",
        "zizaco/entrust": "5.2.x-dev",
        "roumen/sitemap": "dev-master",
        "cviebrock/image-validator": "dev-master",
        "anahkiasen/former": "^4.0",
        "laravelcollective/html": "~5.0",
        "barryvdh/laravel-debugbar": "~2.1.3",
        "php": ">=5.5.9",
        "graham-campbell/throttle": "^5.2",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/general.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
